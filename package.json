{
  "name": "@lit/currency-conversion-js",
  "private": true,
  "version": "1.0.0",
  "description": "A simple currency conversion application",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.js",
    "cypress:open": "cypress open",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "lint": "eslint '**/*.js'",
    "serve": "wds --watch --open",
    "start:build": "npm run build && es-dev-server --root-dir dist --app-index index.html --compatibility none --open",
    "test": "wtr",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "javascript",
    "lit"
  ],
  "author": "Tushar Sanan",
  "license": "Tushar Sanan",
  "dependencies": {
    "@lion/ajax": "^0.11.0",
    "@lion/core": "^0.17.0",
    "@lion/fieldset": "^0.19.1",
    "@lion/form": "^0.11.1",
    "@lion/form-core": "^0.12.0",
    "@lion/input-amount": "^0.13.1",
    "@lion/select": "^0.12.0",
    "lit": "^2.0.0-rc.1",
    "lit-element": "^3.0.0-rc.1"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.1.5",
    "@open-wc/building-rollup": "^1.10.0",
    "@open-wc/testing": "^2.5.32",
    "@open-wc/testing-karma": "^4.0.9",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/mocha": "^7.0.2",
    "@web/dev-server": "0.0.29",
    "@web/dev-server-legacy": "^0.1.4",
    "@web/dev-server-rollup": "^0.2.7",
    "@web/test-runner": "^0.12.15",
    "@web/test-runner-mocha": "^0.3.5",
    "@web/test-runner-playwright": "^0.8.4",
    "babel-eslint": "^10.1.0",
    "cypress": "^7.3.0",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-lit-a11y": "^1.0.1",
    "lit-analyzer": "^1.1.10",
    "mocha": "^7.1.1",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "sinon": "^10.0.0"
  }
}
